[program:jms_job_queue_runner:admin]
command=/usr/bin/php /vagrant/htdocs/admin/app/console jms-job-queue:run --verbose --env=prod
numprocs=1
directory=/tmp
autostart=true
autorestart=true
startsecs=5
startretries=10
user=www-data
redirect_stderr=false
stderr_logfile=/var/log/job-queue.err.log
stdout_logfile=/var/log/job-queue.out.log
stdout_capture_maxbytes=1MB
stderr_capture_maxbytes=1MB
